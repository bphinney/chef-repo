[/opt/tomcat/logs/activfoundation.log]
tags: activfoundation
type: activfoundation 
add_field: environment,<%= node.chef_environment %>
# Following regex is the %{TIME} pattern extruded from the grok-patterns
# file. Beaver multiline matches lines consecutively, so this will multiline
# exceptions from timestamp to timestamp.
multiline_regex_before = ^((?!^(?!<[0-9])(?:2[0123]|[01]?[0-9]):(?:[0-5][0-9])(?::(?:(?:[0-5]?[0-9]|60)(?:[:.,][0-9]+)?))(?![0-9])).)*$
exclude: \.gz

