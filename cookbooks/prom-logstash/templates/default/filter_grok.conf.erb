          }
        }

        if [type] == "prworld" {
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{DATE} %{TIME} %{THREAD:thread} %{LOGLEVEL:loglevel}%{SPACE}%{JAVACLASS:class} - %{GREEDYDATA}" }
          }
        }

}
