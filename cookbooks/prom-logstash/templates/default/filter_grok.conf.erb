        if [type] == "foundation-report" {
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{TIME} %{THREAD:thread} %{LOGLEVEL:loglevel}%{SPACE}%{JAVACLASS:class} - %{GREEDYDATA}" }
          }
        }

        if [type] == "catalina" {
          # Catalina
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{MONTHDAY}-%{MONTH}-%{YEAR} %{TIME} %{LOGLEVEL:loglevel}%{SPACE}%{THREAD:thread} %{JAVACLASS:class} %{GREEDYDATA}" }
          }
          # Spring Initialization
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{SYSLOGPROG} \|-%{LOGLEVEL:loglevel} in %{JAVACLASS:javaclass}%{GREEDYDATA}" }
          }                   
        }

        if [type] == "foundation-management" {
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{TIME} %{THREAD:thread} %{LOGLEVEL:loglevel}%{SPACE}%{JAVACLASS:class} - %{GREEDYDATA}" }
          }
        }

        if [type] == "collabserver" {
