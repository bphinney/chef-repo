filter {
  
        if [type] == "apache" {
          date {
            match => [ "timestamp", "dd/MMM/yyyy:HH:mm:ss Z" ]
          }
          # Apache access log formatting
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{COMBINEDAPACHELOG}" }
            tag_on_failure => [ ]
          }
          # Apache error log formatting
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{HTTPD20_ERRORLOG}" }
            tag_on_failure => [ ]
          }
        }

        if [type] == "activfoundation" {
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{TIME} %{THREAD:thread} %{LOGLEVEL:loglevel}%{SPACE}%{JAVACLASS:class} - %{GREEDYDATA}" }
          }
        }

        if [type] == "foundation-async" {
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{TIME} %{THREAD:thread} %{LOGLEVEL:loglevel}%{SPACE}%{JAVACLASS:class} - %{GREEDYDATA}" }
          }
        }

        if [type] == "flipchart-integration" {
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{TIME} %{THREAD:thread} %{LOGLEVEL:loglevel}%{SPACE}%{JAVACLASS:class} - %{GREEDYDATA}" }
          }
        }

        if [type] == "foundation-load" {
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{TIME} %{THREAD:thread} %{LOGLEVEL:loglevel}%{SPACE}%{JAVACLASS:class} - %{GREEDYDATA}" }
          }
        }

        if [type] == "foundation-maint" {
          grok {
            patterns_dir => "/etc/logstash/patterns"
            match => { "message" => "%{TIME} %{THREAD:thread} %{LOGLEVEL:loglevel}%{SPACE}%{JAVACLASS:class} - %{GREEDYDATA}" }
